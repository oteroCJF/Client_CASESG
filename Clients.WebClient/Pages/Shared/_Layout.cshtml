    <script>
        $(function () {
            $(".submodulo").click(function () {
                Swal.fire({
                    allowOutsideClick: false,
                    icon: "info",
                    title: 'Módulo de ' + $(this).data("modulo"),
                    html: "Accesando a <b>" + $(this).data("submodulo") + "</b>, por favor espere."
                })
                Swal.showLoading();
            });
        });
    </script>
